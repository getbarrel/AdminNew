}
$Contents .="			<tr><td colspan='".($_SESSION["admininfo"]["admin_level"] == 9 ? ($list_type == "incom_after_cancel" || $list_type == "claim" ? "10" : "10") : ($list_type == "incom_after_cancel" || $list_type == "claim" ? "9" : "9"))."' class=dot-x></td></tr>";

