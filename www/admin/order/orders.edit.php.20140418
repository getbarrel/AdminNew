													<td class='input_box_item' colspan='3' style='padding:10px;'>";
											if(is_array($info_history)){
												foreach($info_history as  $i_h){
													$Contents .= $i_h[regdate]." - ".getOrderStatus($i_h[status])." -".$i_h[status_message]." - ".$i_h[admin_message]."<br/>";
												}
