function PrintOrderSummary(){
	global $db, $admininfo;
	if($_SESSION["admininfo"][admin_level] == 9){
		if($db->dbms_type == "oracle"){
			$sql = "SELECT o.oid, o.bname, o.method, o.date_ as regdate, o.payment_price, COUNT(od.oid) AS od_cnt
				FROM ".TBL_SHOP_ORDER." o INNER JOIN ".TBL_SHOP_ORDER_DETAIL." od ON od.oid = o.oid
				WHERE rownum < 7 and  o.status != 'SR'
				GROUP BY o.oid, o.bname, o.method, o.date_
				ORDER BY o.date_ ";
		}else{
			$sql = 'SELECT o.oid, o.bname, o.method, o.date as regdate, o.payment_price,  COUNT(od.oid) AS od_cnt
				FROM '.TBL_SHOP_ORDER.' o INNER JOIN '.TBL_SHOP_ORDER_DETAIL.' od ON od.oid = o.oid
				WHERE  o.status != "SR"
				GROUP BY o.oid
				ORDER BY o.date DESC
				LIMIT 0,7';
